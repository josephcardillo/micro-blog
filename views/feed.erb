<%= erb :header %>

<div class="container1">
    <h1>Feed</h1>
      <% @posts.each do |post| %>

        <h2><a href="/post/<%= post.id %>"><%= post.title %></a></h2>
          <div class="postBackGround">
            <p>Post: <%= post.content %></p>
            <p>By: <a href="/user/<%= post.user.username %>"><%= post.user.username %></a></p>
          </div>
        <% end %>

</div>
